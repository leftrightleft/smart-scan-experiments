+      def time_diff_in_days(timestamp)
+        (::Date.today - ::Time.at(timestamp.to_i).to_date).to_i
+      end
+
+      def unix_time_to_iso8601(timestamp)
+        Time.at(timestamp.to_i).iso8601
+      end
     end
