+  describe "Fails to parse" do
+    let(:config) do
+      <<-CONFIG
+      filter {
+        azure_event {
+
+        }
+      }
+      CONFIG
+    end
+
+    sample("{'a': 'b'}") do
+      expect(subject).to include("tags")
+      expect(subject.get("[tags][0]")).to eq("_azure_event_failure")
+    end
+  end
+
+end
