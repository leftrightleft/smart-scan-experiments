+        final String append = getResultPath(temp);
+        Append.main(resourcePath(String.format("ingest%s.json", testCase)), append);
+        assertThat(
+            utf8File(append), is(utf8File(resourcePath(String.format("logstash%s.conf", testCase))))
+        );
+    }
