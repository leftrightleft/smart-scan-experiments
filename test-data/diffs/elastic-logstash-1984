-    @last_flush.value = 0
-
-    if should_clear?
-      #Reset counter since metrics were cleared
-      @last_clear.value = 0
-      @metric_meters.clear
-      @metric_timers.clear
-    end
-
-    filter_matched(event)
-    return [event]
-  end
