-      #line.chomp!
-      k,v = line.scan(AUTHFILEREGEX).flatten
-      if k and v
-        @logger.debug("Added authfile entry '#{k}' with key '#{v}'")
-        @auth[k] = v
-      else
-        @logger.info("Ignoring malformed authfile line '#{line.chomp}'")
-      end
-    end
-  end # def parse_authfile
