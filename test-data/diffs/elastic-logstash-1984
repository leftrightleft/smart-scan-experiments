-  INTERVAL_VALUES_FIELDS = {
-    "interval" => true,
-    "values" => true,
-  }
-
-  INTERVAL_BASE_FIELDS = {
-    'host' => true,
-    'collectd_type' => true,
-    'plugin' => true,
-    'plugin_instance' => true,
-    '@timestamp' => true,
-    'type_instance' => true,
-  }
-
-  INTERVAL_TYPES = {
-    7 => true,
-    9 => true,
