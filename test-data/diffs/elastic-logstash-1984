-  def should_flush?
-    @last_flush.value >= @flush_interval && (!@metric_meters.empty? || !@metric_timers.empty?)
-  end
-
-  def should_clear?
-    @clear_interval > 0 && @last_clear.value >= @clear_interval
-  end
