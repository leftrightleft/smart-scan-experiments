-      # TODO: might be better to V1_TO_V0 = V0_TO_V1.invert during
-      # initialization than V0_TO_V1.has_value? within loop
-      next if field == "@version" or V0_TO_V1.has_value?(field)
-      h["@fields"] ||= {}
-      h["@fields"][field] = val
-    end
-
