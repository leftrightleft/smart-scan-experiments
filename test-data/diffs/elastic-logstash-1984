-  def parse_authfile
-    # We keep the authfile parsed in memory so we don't have to open the file
-    # for every event.
-    @logger.debug("Parsing authfile #{@authfile}")
-    if !File.exist?(@authfile)
-      raise "The file #{@authfile} was not found"
-    end
-    @auth.clear
