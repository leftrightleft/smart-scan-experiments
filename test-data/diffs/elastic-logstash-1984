-        @logger.debug("Definition complete", :field => field)
-        [field]
-      else
-        @logger.warn("Definition should be an array", :field => field)
-        nil
-      end
-    else
-      @logger.warn("Unsupported field", :type => type, :length => length)
-      nil
-    end
-  end # def netflow_field_for
