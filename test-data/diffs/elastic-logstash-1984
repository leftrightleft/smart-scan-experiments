-    8               => "@timestamp",
-    9               => "interval",
-    256             => "message",
-    257             => "severity",
-    SIGNATURE_TYPE  => "signature",
-    ENCRYPTION_TYPE => "encryption"
-  }
-
-  PLUGIN_TYPE_FIELDS = {
-    'host' => true,
-    '@timestamp' => true,
-  }
-
-  COLLECTD_TYPE_FIELDS = {
-    'host' => true,
-    '@timestamp' => true,
-    'plugin' => true,
-    'plugin_instance' => true,
-    'type_instance' => true,
-  }
