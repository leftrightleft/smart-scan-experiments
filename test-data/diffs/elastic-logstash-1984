-  # Map from v0 name to v1 name.
-  # Note: @source is gone and has no similar field.
-  V0_TO_V1 = {"@timestamp" => "@timestamp", "@message" => "message",
-              "@tags" => "tags", "@type" => "type",
-              "@source_host" => "host", "@source_path" => "path"}
