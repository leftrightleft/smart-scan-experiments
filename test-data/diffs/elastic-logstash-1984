-  private
-  def match(groks, field, event)
-    input = event[field]
-    if input.is_a?(Array)
-      success = false
-      input.each do |input|
-        success |= match_against_groks(groks, input, event)
-      end
-      return success
-    else
-      return match_against_groks(groks, input, event)
-    end
-  rescue StandardError => e
-    @logger.warn("Grok regexp threw exception", :exception => e.message)
