-# encoding: utf-8
-require "logstash/filters/base"
-require "logstash/namespace"
-
-# Drop filter.
-#
-# Drops everything that gets to this filter.
-#
-# This is best used in combination with conditionals, for example:
-#
-#     filter {
-#       if [loglevel] == "debug" { 
-#         drop { } 
-#       }
-#     }
-#
